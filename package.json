{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "lint": "eslint --fix -f unix \"src/**/*.{ts,tsx}\" && prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "clean-yarn-cache": "yarn cache clean",
    "remove-build": "rimraf dist",
    "build": "tsc && yarn run remove-build && cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.config.ts",
    "build-dev": "yarn run build --mode development",
    "start": "serve -s dist",
    "publish": "gh-pages -b deploy -d dist",
    "build-and-publish": "yarn run build && yarn run publish",
    "watch": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack serve --config webpack.config.ts --mode development",
    "check": "ncu",
    "update": "ncu -u && yarn",
    "npm-check": "npm-check",
    "audit-ci": "audit-ci",
    "audit": "yarn start | lighthouse http://localhost:5000 --output-path ./lighthouse.html --view",
    "build-and-start": "yarn run build && yarn start",
    "build-img": "docker build . -t portfolio",
    "start-img": "docker run -p 5000:5000 portfolio",
    "build-dev-img": "docker build . -t portfolio-dev -f Dockerfile.dev",
    "start-dev-img": "docker run -p 5000:5000 -it portfolio-dev",
    "build-img-podman": "podman build . -t portfolio",
    "start-img-podman": "podman run -p 5000:5000 portfolio",
    "build-dev-img-podman": "podman build . -t portfolio-dev -f Dockerfile.dev",
    "start-dev-img-podman": "podman run -p 5000:5000 -it portfolio-dev"
  },
